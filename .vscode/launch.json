{
  "version": "0.2.0",
  "configurations": [
    {
      "name": ".NET Core Launch (web) - AuthService",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build AuthService",
      "program": "${workspaceFolder}/src/Services/AuthService/AuthService/bin/Debug/net9.0/AuthService.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Services/AuthService/AuthService",
      "stopAtEntry": false,
      "serverReadyAction": {
        "action": "openExternally",
        "pattern": "Now listening on: (https?://.*)"
      },
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "sourceFileMap": {
        "/Views": "${workspaceFolder}/Views"
      }
    }
  ]
}
